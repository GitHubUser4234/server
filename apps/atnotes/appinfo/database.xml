				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>1</length>
			</field>
			<field>
				<name>is_shared</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>1</length>
			</field>
			<index>
				<name>note_pk</name>
			    <primary>true</primary>
			    <field>
					<name>note_id</name>
					<sorting>ascending</sorting>
				</field>
			</index>
		</declaration>
	</table>
</database>