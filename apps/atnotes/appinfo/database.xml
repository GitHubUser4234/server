				<autoincrement>1</autoincrement>
				<unsigned>true</unsigned>
				<length>4</length>
			</field>
			<field>
				<name>oc_uid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>create_ts</name>
				<type>integer</type>
				<notnull>true</notnull>
				<length>11</length>
			</field>
			<field>
				<name>update_ts</name>
				<type>integer</type>
				<notnull>false</notnull>
				<default>0</default>
				<length>11</length>
			</field>
			<field>
				<name>note_title</name>
				<type>text</type>
				<notnull>true</notnull>
